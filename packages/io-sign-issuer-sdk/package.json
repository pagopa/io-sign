{
  "name": "@io-sign/io-sign-issuer-sdk",
  "version": "",
  "description": "Issuer SDK for io-sign",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagopa/io-sign"
  },
  "keywords": [
    "fetch",
    "typescript",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "Unlicense",
  "main": "./dist/index.js",
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js"
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start-cli": "node dist/index.js",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "build": "tsc",
    "generate:api-client": "npx @openapitools/openapi-generator-cli generate -i openapi.yaml -g typescript -o ../io-sign-api-client"
  },
  "dependencies": {
    "@io-sign/io-sign-api-client": "^0.1.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.18",
    "dotenv": "^16.0.3",
    "es6-promise": "^4.2.4",
    "inquirer": "^8.0.0",
    "url-parse": "^1.4.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/url-parse": "1.4.4",
    "typescript": "^4.0"
  }
}
