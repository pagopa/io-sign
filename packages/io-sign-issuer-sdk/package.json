{
  "name": "io-sign-issuer-yaml",
  "version": "1.0.0",
  "description": "Issuer SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagopa/io-sign"
  },
  "license": "Unlicense",
  "main": "./dist/index.js",
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js"
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start-cli": "dotenv node dist/index.js",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "build": "tsc",
    "generate:api-client": "npx @openapitools/openapi-generator-cli generate -i openapi.yaml -g typescript-fetch -o ../io-sign-api-client --additional-properties=npmName=@io-sign/io-sign-api-client"
  },
  "dependencies": {
    "@io-sign/io-sign-api-client": "^1.0.0",
    "dotenv": "^16.0.3",
    "inquirer": "^8.0.3",
    "@types/node": "^18.15.3",
    "isomorphic-fetch": "^3.0.0",
    "read-yaml-file": "^2.1.0",
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@pagopa/eslint-config": "^3.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/inquirer": "^8.0.3",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.11.18",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4"
  }
}
