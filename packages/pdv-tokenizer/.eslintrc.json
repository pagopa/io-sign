{
  "env": {
    "es2021": true,
    "node": true
  },
  "ignorePatterns": ["Dangerfile.js", "src/models"],
  "extends": ["@pagopa/eslint-config/recommended"],
  "overrides": [
    {
      "files": ["**/*.spec.ts"],
      "rules": {
        "functional/no-let": "off"
      }
    }
  ],
  "rules": {
    "max-classes-per-file": "off"
  }
}
