{
  "name": "@io-sign/io-sign",
  "version": "1.4.1",
  "repository": "https://github.com/pagopa/io-sign",
  "license": "MPL-2.0",
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@azure/event-hubs": "catalog:",
    "@azure/storage-blob": "catalog:",
    "@azure/storage-queue": "catalog:",
    "@pagopa/handler-kit": "^1.1.0",
    "@pagopa/io-functions-commons": "catalog:",
    "@pagopa/io-functions-services-sdk": "catalog:",
    "@pagopa/logger": "catalog:",
    "@pagopa/ts-commons": "catalog:",
    "applicationinsights": "catalog:",
    "date-fns": "catalog:",
    "fp-ts": "catalog:",
    "handler-kit-legacy": "catalog:",
    "io-ts": "catalog:",
    "io-ts-types": "catalog:",
    "monocle-ts": "catalog:",
    "newtype-ts": "catalog:",
    "pdf-lib": "catalog:",
    "ulid": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@io-sign/eslint-config": "workspace:^",
    "@pagopa/openapi-codegen-ts": "catalog:",
    "@vitest/coverage-c8": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "^4.8.4",
    "vitest": "catalog:"
  },
  "scripts": {
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "build": "tsc",
    "generate:api-models": "gen-api-models --api-spec ./src/infra/pdv-tokenizer/openapi.yaml --out-dir ./src/infra/pdv-tokenizer/models --client --request-types --response-decoders"
  },
  "stableVersion": "0.9.0-beta.3"
}
