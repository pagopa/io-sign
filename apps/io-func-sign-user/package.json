{
  "name": "io-func-sign-user",
  "version": "0.3.1",
  "private": true,
  "files": [
    "**/function.json",
    "dist",
    "host.json",
    "extensions.csproj"
  ],
  "bundledDependencies": true,
  "scripts": {
    "build": "tsup-node",
    "format": "prettier --write .",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc",
    "start": "func start  --port 7072",
    "build:package": "npm-pack-zip --add-version",
    "generate:lollipop-api-models": "rimraf ./src/infra/lollipop/models && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions-lollipop/f0a4485bf65f4f2d50ca1383a56a808c4f2ad4ea/openapi/external.yaml --no-strict --out-dir ./src/infra/lollipop/models --request-types --response-decoders --client",
    "generate:user-api-models": "rimraf ./src/infra/http/models && gen-api-models --api-spec ./openapi.yaml --out-dir ./src/infra/http/models",
    "generate:api-models": "yarn generate:user-api-models & yarn generate:lollipop-api-models"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.2",
    "@azure/storage-blob": "^12.12.0",
    "@azure/storage-queue": "^12.11.0",
    "@io-sign/io-sign": "^1.0.0",
    "@pagopa/handler-kit": "^1.0.1",
    "@pagopa/handler-kit-azure-func": "^1.2.0",
    "@pagopa/io-functions-commons": "^26.2.1",
    "@pagopa/logger": "1.0.1",
    "@pagopa/ts-commons": "^10.14.2",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "fp-ts": "^2.13.1",
    "handler-kit-legacy": "npm:@pagopa/handler-kit@0.5.1",
    "io-ts": "^2.2.19",
    "io-ts-types": "^0.5.19",
    "jose": "^4.13.1",
    "jsrsasign": "^10.6.1",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "rimraf": "^4.4.0"
  },
  "devDependencies": {
    "@azure/cosmos": "^3.17.2",
    "@pagopa/eslint-config": "^3.0.0",
    "@pagopa/openapi-codegen-ts": "^12.2.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jsrsasign": "^10.5.4",
    "@types/node-jose": "^1.1.10",
    "@vitest/coverage-c8": "^0.30.1",
    "azure-functions-core-tools": "^4.0.4895",
    "eslint": "^8.28.0",
    "npm-pack-zip": "^1.3.0",
    "prettier": "2.7.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "vitest": "^0.30.1"
  }
}
