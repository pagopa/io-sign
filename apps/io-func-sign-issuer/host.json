{
  "version": "2.0",

  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.3.*, 4.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix": "api/v1/sign"
    },
    "tracing": {
      "traceInputsAndOutputs": false,
      "traceReplayEvents": false
    },
    "queues": {
      "maxPollingInterval": "00:00:05.000"
    }
  }
}
