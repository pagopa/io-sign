{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "signature-requests/{signatureRequestId}/notification",
      "methods": ["put"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ],
  "scriptFile": "../dist/main.js",
  "entryPoint": "SendNotification"
}
