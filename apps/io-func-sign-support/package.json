    "@types/node": "^18.15.5",
    "@vitest/coverage-c8": "^0.30.1",
    "azure-functions-core-tools": "^4.0.5148",
    "eslint": "^8.28.0",
    "npm-pack-zip": "^1.3.0",
    "prettier": "2.7.1",
    "tsup": "^6.6.3",
    "typescript": "^4.8.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.1",
    "@io-sign/io-sign": "workspace:^",
