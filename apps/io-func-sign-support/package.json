  "files": [
    "**/function.json",
    "dist",
    "host.json",
    "extensions.csproj"
  ],
  "bundledDependencies": true,
  "scripts": {
    "build": "tsup-node",
    "format": "prettier --write .",
    "lint": "eslint \"src/**\"",
